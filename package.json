  "homepage": "https://github.com/Appleec/e-utils#readme",
  "devDependencies": {
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/chai": "^4.3.17",
    "@types/jest": "^29.5.12",
    "bumpp": "^9.5.2",
    "chai": "^4.5.0",
    "esbuild": "^0.23.1",
    "jest": "^29.7.0",
    "rollup": "^4.21.0",
    "rollup-plugin-dts": "^6.1.1",
